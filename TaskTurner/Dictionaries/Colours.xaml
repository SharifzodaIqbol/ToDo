<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="AccentDark" Color="#FF164863"/>
    <SolidColorBrush x:Key="AccentDarkOver" Color="#FF164863"/>
    <SolidColorBrush x:Key="AccentDarkClick" Color="#FF5097BD"/>
    <SolidColorBrush x:Key="TaskListBackground" Color="#FFECECEC"/>
    <SolidColorBrush x:Key="WindowBackground" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="TaskStatusBackground" Color="#FFF6F6F6"/>

    <!--OnAccent-->
    <SolidColorBrush x:Key="OnAccent" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="Borders" Color="#FFC9B5B5"/>

    <!--Importance Colours-->
    <SolidColorBrush x:Key="Importance-Low" Color="#FF0CAFF4"/>
    <SolidColorBrush x:Key="Importance-Med" Color="#FF2AD985"/>
    <SolidColorBrush x:Key="Importance-High" Color="#FFF4830C"/>
    <SolidColorBrush x:Key="Importance-Crit" Color="#FFCA2C2C"/>
    
    <!--Task Progress-->
    <SolidColorBrush x:Key="Progress InProgress" Color="#FF518657"/>
    <SolidColorBrush x:Key="Progress Completed" Color="#FF4187AC"/>
    <SolidColorBrush x:Key="Progress NotStarted" Color="#FFB1B1B1"/>
    <SolidColorBrush x:Key="Progress Late" Color="#FF846161"/>
    <SolidColorBrush x:Key="Progress Archived" Color="#FFCBA36C"/>
    <SolidColorBrush x:Key="Progress Deleted" Color="#FF606060"/>
</ResourceDictionary>