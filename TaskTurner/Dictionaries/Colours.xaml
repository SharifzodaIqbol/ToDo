    <SolidColorBrush x:Key="Button-Complate-Click" Color="#FF6BA98B"/>
    <SolidColorBrush x:Key="Button-Edit" Color="#FFF4830C"/>
    <SolidColorBrush x:Key="Button-Edit-Over" Color="#FFD19353"/>
    <SolidColorBrush x:Key="Button-Edit-Click" Color="#FFBA9166"/>
    <SolidColorBrush x:Key="Button-Delete" Color="#FFCA2C2C"/>
    <SolidColorBrush x:Key="Button-Delete-Over" Color="#FFDA5A5A"/>
    <SolidColorBrush x:Key="Button-Delete-Click" Color="#FFAE5151"/>
    <!--Task Progress-->
    <SolidColorBrush x:Key="Progress InProgress" Color="#FF518657"/>
    <SolidColorBrush x:Key="Progress Completed" Color="#FF4187AC"/>
    <SolidColorBrush x:Key="Progress NotStarted" Color="#FFB1B1B1"/>
    <SolidColorBrush x:Key="Progress Late" Color="#FF846161"/>
    <SolidColorBrush x:Key="Progress Archived" Color="#FFCBA36C"/>
    <SolidColorBrush x:Key="Progress Deleted" Color="#FF606060"/>
